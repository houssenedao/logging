{
    "name": "mars/logging",
    "description": "Mars PHP framework logging package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Houssene Dao",
            "email": "dao.houssene@themartiangeeks.com"
        },
        {
            "name": "Hassane Dao",
            "email": "dao.hassane@themartiangeeks.com"
        }
    ],
    "support": {
        "issues": "https://github.com/marsphp/logging/issues",
        "source": "https://github.com/marsphp/logging"
    },
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Mars\\Logging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
